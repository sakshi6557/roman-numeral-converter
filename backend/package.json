{
  "name": "roman-numeral-converter-backend",
  "version": "1.0.0",
  "description": "Backend for Roman Numeral Converter",
  "main": "dist/app.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production node dist/app.js",
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development nodemon",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docs": "typedoc --options ../typedoc.json",
    "docs:clean": "rimraf docs",
    "docs:build": "npm run docs:clean && npm run docs",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "prom-client": "^14.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.30",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.8",
    "typescript": "^5.3.3"
  }
}
